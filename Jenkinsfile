pipeline {
    agent any

    stages {
        stage('Clone repository') {
            steps {
                echo "Starting"
            }
        }

        stage('Build Docker Image') {
            steps {
                script {
                    echo "seting project"
                }
            }
        }

        stage('Test') {
            steps {
                echo "Testing"
            }
        }

        stage('Push Docker Image') {
            steps {
                echo "up stage"
            }
        }

        stage('Deploy') {
            steps {
                echo "deployment"
            }
        }
    }

}
